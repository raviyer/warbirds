{
.echo @PA_count@ PA Parked a/c types - various types specific to terrains. remain
}
if (@FD_count@ > 0)
{
.echo @FD_count@ FD Small fuel tanks at airfields and other locations. remain
}
if (@AD_count@ > 0)
{
.echo @AD_count@ AD Various fortified ammunition storage bunkers at airfields and tactical positions. remain
}
if (@G1_count@ > 0)
{
.echo @G1_count@ G1 Twin MG position with man. remain
}
if (@G0_count@ > 0)
{
.echo @G0_count@ G0 Large flak gun in sandbag ring. remain
}
if (@HS_count@ > 0)
