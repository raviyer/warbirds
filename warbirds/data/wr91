}
if (@HQ_count@ > 0)
{
.echo @HQ_count@ HQ Any building with special significance. remain
}
if (@G3_count@ > 0)
{
.echo @G3_count@ G3 Medium flak postion in dirt ring OR large sandbag ring with wheeled gun. remain
}
if (@HG_count@ > 0)
{
.echo @HG_count@ HG Airfield Structures of various types and sizes to house aircraft. remain
}
if (@G2_count@ > 0)
{
.echo @G2_count@ G2 Small flak position in dirt ring OR medium sandbag ring with MG and man. remain
}

